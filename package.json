{
	"name": "tugboata",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "npx react-native start --reset-cache",
		"start:8082": "npx react-native start --reset-cache --port 8082",
		"uninstall": "adb uninstall com.tugboata",
		"an": "npx react-native run-android --terminal APP_ENV=local",
		"an:who": "adb devices",
		"an:emu": "emulator -avd Pixel_9_Pro_API_35 && npx react-native run-android --port 8082",
		"an:clean": "cd android 2>/dev/null && ./gradlew clean || (cd .. && cd android && ./gradlew clean)",
		"an:build-install": "yarn build:apk && adb install -r android/app/build/outputs/apk/release/app-release.apk",
		"ios": "npx react-native run-ios APP_ENV=local",
		"ios:16": "npx react-native run-ios --port 8082 --simulator='iPhone 16'",
		"ios:16pro": "npx react-native run-ios --port 8082 --simulator='iPhone 16 Pro'",
		"pod": "cd ios 2>/dev/null && pod install || (cd .. && cd ios && pod install)",
		"pod:clean": "cd ios 2>/dev/null && pod cache clean --all || (cd .. && cd ios && pod cache clean --all)",
		"pod:update": "cd ios 2>/dev/null && pod repo update || (cd .. && cd ios && pod repo update)",
		"pod:delete": "cd ios 2>/dev/null && pod deintegrate || (cd .. && cd ios && pod deintegrate)",
		"xcode": "open ios/FourIdioms.xcworkspace",
		"xcode:clean": "rm -rf ~/Library/Developer/Xcode/DerivedData",
		"yarn:clean": "yarn cache clean",
		"metro:clean": "npx react-native start --reset-cache",
		"babel:clean": "rimraf node_modules/.cache/babel-loader/*,",
		"lint:clean": "eslint --cache --cache-location node_modules/.cache/eslint/ --cache-file .",
		"watchman:clean": "watchman shutdown-server",
		"build:apk": "cd android 2>/dev/null && ./gradlew assembleRelease || (cd .. && cd android && ./gradlew assembleRelease)",
		"build:aab": "cd android 2>/dev/null && ./gradlew bundleRelease || (cd .. && cd android && ./gradlew bundleRelease)"
	},
